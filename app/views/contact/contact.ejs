  <div class="foo-padding container">
    <h1 class="title text-center">Contato</h1>
    <div class="row-fluid justify-content-md-center">
      <form action="/">
        <div class="form-group">
          <label for="inputName">Nome</label>
          <input type="text" class="form-control" id="inputName" placeholder="Digite seu nome">
        </div>
        <div class="form-group">
          <label for="inputEmail">E-mail</label>
          <input type="email" class="form-control" id="inputEmail" placeholder="Digite seu e-mail">
        </div>
        <div class="form-group">
          <label for="inputSubject">Assunto</label>
          <input type="text" class="form-control" id="inputSubject" placeholder="Assunto">
        </div>
        <div class="form-group">
          <label for="message"></label>
          <textarea class="form-control" name="message" id="message" cols="30" rows="10" placeholder="Escreva sua mensagem"></textarea>
        </div>
        <button type="submit" class="btn btn-form">Enviar</button>
      </form>
    </div>
  </div>
  <footer class="foo">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-12 col-md-5 brand">
